
<% if (visits) { %>
      
    <!--<input class="myInput" type="text" placeholder="Search..">-->
    <container class="userlist-container">
    
        <% for(var i=0; i < visits.length; i++) { %>
            <ul class="userlist">
            <li>
                Date: <%= visits[i].date %>
            </li>
            <li>
                Activity: <%= visits[i].activitylist %>
            </li>
            <li>
                Last Name: <%= visits[i].lastname %>
            </li>
            <li>
                    Approved: <%= visits[i].approved %>
                </li>
            
            <a href="approvalform/<%=visits[i]._id %>"><button class="validatevisit">Validate</button></a>
            <button class="deletevisit">Remove</button>
        </ul>
            <% } %>
    
</container>
    <% } %>
